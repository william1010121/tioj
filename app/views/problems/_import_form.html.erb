
<%=form_with url: import_problem_problems_path, method: :post, html: {class: 'form', id: 'import-problem-form'}, authenticity_token: true do |f| %>

<div class='form-group'>
    <%= f.label :problem_data, "Problem data"%>
    <%= f.file_field :problem_data, :class => 'form-control flat'%>
</div>
<div class='form-group'>
    <%= f.label :data_type, "data type"%><br>
    <%= f.select :data_type,  options_for_select(['zjson', 'other']), {id: 'data-type'}, class: 'form-control' %>
</div>
<%= f.submit :class => 'btn btn-success btn-lg', :id => 'submit-btn'%>
<% end %>
